# Goodfire API Configuration
# Get your API key from https://platform.goodfire.ai
E2E_GOODFIRE_API_KEY=your_goodfire_api_key_here

# Goodfire SDK Client Base URL
# This is typically provided by your Goodfire administrator
E2E_GOODFIRE_SDK_CLIENT_BASE_URL=https://api.goodfire.ai

# Anthropic API Key
# Required for Claude model testing
# Get your API key from https://console.anthropic.com/
E2E_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Concurrent Requests Configuration
# Number of concurrent requests for testing (default: 32)
E2E_CONCURRENT_REQUESTS=32
